{
  "manifest_version": 3,
  "name": "add blocker",
  "version": "1.4",
  "permissions": [
    "cookies",
    "tabs",
    "alarms",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
